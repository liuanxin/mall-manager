{
  "name": "mall-manager",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "export NODE_OPTIONS=--openssl-legacy-provider; vue-cli-service serve",
    "test": "export NODE_OPTIONS=--openssl-legacy-provider; vue-cli-service build --mode staging",
    "build": "export NODE_OPTIONS=--openssl-legacy-provider; vue-cli-service build",
    "lint": "export NODE_OPTIONS=--openssl-legacy-provider; eslint --ext .js,.vue src --fix",
