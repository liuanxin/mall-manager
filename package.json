  },
  "dependencies": {
    "axios": "0.27.2",
    "core-js": "3.22.5",
    "element-ui": "2.15.8",
    "js-cookie": "3.0.1",
    "lodash": "4.17.21",
    "normalize.css": "8.0.1",
    "nprogress": "0.2.0",
    "path-to-regexp": "3.2.0",
    "vue": "2.6.14",
    "vue-router": "3.5.3",
    "vue-template-compiler": "2.6.14",
    "vuex": "3.6.2"
  },
  "devDependencies": {
    "@babel/core": "7.17.9",
    "@babel/eslint-parser": "7.17.0",
    "@vue/babel-preset-app": "5.0.4",
    "@vue/cli-plugin-babel": "4.5.17",
    "@vue/cli-service": "4.5.17",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "3.4.1",
    "eslint-plugin-prettier-vue": "3.1.0",
    "eslint-plugin-vue": "7.20.0",
    "prettier": "2.6.2",
    "sass": "1.51.0",
    "sass-loader": "10.2.1",
    "svg-sprite-loader": "6.0.11",
    "svgo": "2.8.0",
    "webpack": "5.72.1"
  },
  "engines": {
    "node": ">= 8.9",
    "npm": ">= 3.0.0"
